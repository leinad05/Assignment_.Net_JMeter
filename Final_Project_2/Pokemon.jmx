              <stringProp name="script">import groovy.json.JsonSlurper

def jsonSlurper = new JsonSlurper()
def currentOffset = vars.get(&quot;offset&quot;) as int
def plusTwnty = (currentOffset + 20) as String

//log.info(&quot;Next offset: ${plusTwnty}&quot;)

def sampleResult = prev
def jsonResponse = sampleResult.getResponseDataAsString()

def pokemonData = jsonSlurper.parseText(jsonResponse)
def pokemonEntry = pokemonData.results.find{ it.name == &quot;pikachu&quot; }

if(pokemonEntry == null) {
	log.info(&quot;pikachu not found!&quot;)
	log.info(&quot;Next offset: ${plusTwnty}&quot;)
	vars.put(&quot;offset&quot;, plusTwnty)
} else {
	vars.put(&quot;pokemonFound&quot;, pokemonEntry.name)
	log.info(&quot;${pokemonEntry.name} found!&quot;)
	vars.put(&quot;detailPokemon&quot;, pokemonEntry.url)
}
</stringProp>
